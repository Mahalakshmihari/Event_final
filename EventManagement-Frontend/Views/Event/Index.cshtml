@model List<EventModel><head>    <link href="~/css/carousel.css" rel="stylesheet" /></head><header>    <nav class="navbar navbar-expand-md navbar-dark fixed-top" style="background-color: #4b0082;">        <a class="navbar-brand" href="#">EventManagementApp</a>        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">            <span class="navbar-toggler-icon"></span>        </button>        <div class="collapse navbar-collapse" id="navbarCollapse">            <ul class="navbar-nav mr-auto">                               <li class="nav-item">                    <a class="nav-link" asp-controller="Booking" asp-action="Index">Booking History</a>                </li>            </ul>            <form class="form-inline mt-2 mt-md-0" asp-controller="Authenticate" asp-action="Registration" method="get">                <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Logout</button>            </form>        </div>    </nav></header><h2 class="header text-center">Discover Our Events</h2>@if (Model == null || !Model.Any()){    <p>No events available at the moment.</p>}else{    @foreach (var eventModel in Model)    {        <br />        <div class="card mb-3">            <img src="@eventModel.ImageURL" class="card-img-top" alt="..." height="400" width="400">            <div class="card-body" style="background:rgb(241, 221, 237)">                <h5 class="card-title" style="text-align:center;text-align:center">@eventModel.EventName</h5>                <p class="card-text">@eventModel.Description</p>                <p style="text-align:center">EVENT DATES FROM : @eventModel.StartDate TO : @eventModel.EndDate</p>                <p style="text-align:center">TICKET PRICE : ₹  @eventModel.Price</p>                <p class="card-text" style="text-align:center"><small class="text-body-secondary">LOCATION : @eventModel.Location</small></p>                <div style="text-align:center;">RATING : @eventModel.Rating   <button class="rating" style="background:green;margin:auto;border:none;border-radius:10px;">  ⭐⭐⭐⭐⭐ </button></div>                <button style="display:block;margin:10px auto;background:skyblue;color:white;border:none;height:40px;width:200px;border-radius:10px;"> <a href="@Url.Action("Index","Seat")" style="text-decoration:none;">Book Ticket</a> </button>                <div><button style="display:block;margin:10px auto;color:skyblue;border:solid 1px blue;height:40px;width:200px;border-radius:10px;"><a class="card-text text-decoration-none" href="@Url.Action("Index","UserReview")">Reviews</a></button></div>            </div>        </div>        <br />        <br />        <br />    }}